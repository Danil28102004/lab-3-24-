operators = {
    "903": "Билайн",
    "905": "Билайн",
    "906": "Билайн",
    "925": "Мегафон",
    "926": "Мегафон",
    "916": "МТС",
    "917": "МТС",
    "910": "МТС",
    "911": "Скайлинк",
    "912": "Теле2",
    "913": "Теле2",
    "914": "Теле2",
    "915": "Теле2",

}

def get_operator(code):
    return operators.get(code, "Неизвестный код")

code = input("Введите код телефона (например, 903): ")
operator = get_operator(code)

print(f"Оператор: {operator}")

---------------------------------------------------
#include <iostream>
#include <string>

int main() {
    int n;
    std::cout << "Введите количество месяцев (n > 12): ";
    std::cin >> n;

    if (n <= 12) {
        std::cout << "Ошибка: n должно быть больше 12." << std::endl;
        return 1;
    }

    const std::string months[] = {
        "Январь", "Февраль", "Март", "Апрель", "Май", "Июнь",
        "Июль", "Август", "Сентябрь", "Октябрь", "Ноябрь", "Декабрь"
    };

    std::cout << "Название месяца: " << months[(n - 1) % 12] << std::endl;

    return 0;
}


